pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'In Building Stage'
                bat javac 'hello world.java'
            }
        }
        stage('Test') {
            steps {
                echo 'In Testing Stage'
                bat javac 'hello world.java'
            }
        }
        stage('Run') {
            steps {
                echo 'In Runing Stage'
                bat javac 'hello world.java'
            }
        }
    }
}
